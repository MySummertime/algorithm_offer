use dp method

  1. create a static array to store each char while we traversing the to_string(num);


  class Solution {
  public:
      int translateNum(int num) {
          std::string str = to_string(num);
          int len = str.length();
          int ans[len + 1];
          ans[0] = 1;
          for (int i = 1; i <= str.size(); ++i) {
              ans[i] = ans[i - 1];
              if (i > 1) {
                  int val = 10 * (str[i - 2] - '0') + (str[i - 1] - '0');
                  if (val < 10 || val > 25) {}
                  else {
                      ans[i] += ans[i - 2];
                  }
              }
          }
          return ans[len];
      }
  };
