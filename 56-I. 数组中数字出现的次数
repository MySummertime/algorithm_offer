use bit operation


  class Solution:
      def singleNumbers(self, nums: List[int]) -> List[int]:
          xorVal = functools.reduce(lambda x, y: x ^ y, nums)
          base = 1
          while not(base & xorVal):
              base <<= 1
          l, r = 0, 0
          for num in nums:
              if base & num:
                  l ^= num
              else:
                  r ^= num
          return [l, r]
